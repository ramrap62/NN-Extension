{
  "name": "NN",
  "version": "5.0",
  "description": "Image Censoring",
  "manifest_version": 2,
  "content_scripts": [{
      "matches": ["<all_urls>"],
      "all_frames": true,
      "js":      ["assets/js/contentScript.js"]
  }],
  "background": {
    "scripts": ["assets/js/background.js"],
    "persistent": false
  },
  "icons": {
    "128": "assets/images/download.png"
  }
}