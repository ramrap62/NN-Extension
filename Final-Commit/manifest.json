{
    "name": "Ad-blocker",
    "description": "Get The sources of the images",
    "version": "1.0",
    "permissions":[
        "activeTab",
        "tabs",
        "notifications",
        "http://*/",
        "https://*/",
        "nativeMessaging",
        "cookies"

    ],
   
    "web_accessible_resources" : [
        "yin.png"],
        "background":
        {
            "scripts": ["background.js"]
        },
        
    "browser_action":{
        "default_title": "Image Source",
        "default_popup":"./Login_v4/Login_v4/popup.html"
        
    },
    "manifest_version": 2 
}